<% if @certficate != false %>
<%      ssl="soap_cacert = /etc/atomiadns-mastercert.pem" %>
<% else %>
<%      ssl=";soap_cacert = /etc/atomiadns-mastercert.pem" %>
<% end %>

soap_uri = <%= @atomiadns_url %>
soap_username = <%= @atomiadns_user %>
soap_password = <%= @atomiadns_password %>
<%= ssl %>
servername = <%= @hostname %>
